            <input asp-for="price" style="text-align: left" class="button" type="number" placeholder="Price">
            <input asp-for="id" type="hidden"/>
            <input asp-for="image" type="hidden"/>
            <input asp-for="sellerId" type="hidden"/>
            <input style="background-color: palevioletred" class="button" type="submit" value="save">
